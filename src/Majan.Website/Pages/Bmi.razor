@code {
    [SupplyParameterFromForm]
    public BMI? Model { get; set; }

    protected override void OnInitialized() => Model ??= new();


    private BMICalculator bmiCalc = new BMICalculator();

    public string? lblBMIOutput { get; set; }
    public string? lblWeightCategoryOutput { get; set; }
    public string? lblBMIMessageOutput { get; set; }
    public string? grpResults { get; set; }
    public string? lblNormalWeightDisplay { get; set; }

    public bool showResults { get; set; } = false;

    private void Submit()
    {
        bool ok = ReadInputBMI();

        if (ok)
        {
            DisplayResults();
        }

    }

    private bool ReadHeight()
    {
        double height = Model!.Height;
        double inchesConverted = 0;




        if ((height > 0))
        {
            if (bmiCalc.GetUnit() == UnitTypes.American)
            {
                bmiCalc.SetHeight((height + inchesConverted) * 12.00);
            }
            else
            {
                bmiCalc.SetHeight(height / 100.0);
            }
        }
        else
        {
            // MessageBox.Show("Invalid Height value!", "Error!");
            return false;
        }

        return true;
    }
    private bool ReadInputBMI()
    {

        bmiCalc.SetUnit(UnitTypes.Metric);


        ReadName();

        return ReadHeight() && ReadWeight();
    }
    private void ReadName()
    {
        if (string.IsNullOrEmpty(Model!.Name))
        {
            bmiCalc.SetName("No name");
        }
        else
        {
            bmiCalc.SetName(Model!.Name);
        }
    }
    private bool ReadWeight()
    {
        double outValue = Model!.Weight;



        if ((outValue > 0))
        {
            bmiCalc.SetWeight(outValue);
        }
        else
        {
            // MessageBox.Show("Invalid Weight value!", "Error!");
            return false;
        }

        return true;
    }


    private void DisplayResults()
    {
        showResults = true;
        lblBMIOutput = bmiCalc.CalculateBMI().ToString("f2");

        lblWeightCategoryOutput = bmiCalc.BMIWeightCategory().ToString();

        lblBMIMessageOutput = "Normal BMI is between 18.50 and 24.9";

        grpResults = "Results for " + bmiCalc.GetName();

        lblNormalWeightDisplay = "Normal weight should be between " + bmiCalc.CalculateWeight(18.50).ToString("f2") + " and " + bmiCalc.CalculateWeight(24.9).ToString("f2") + " kg";

    }




    @code {
        private async Task PrintComponent()
        {
            await Js.InvokeVoidAsync("myInterop.printComponent", "#results");
        }
    }





        public class BMI
        {
    [Required]
    public string? Name { get; set; }
    public double Age { get; set; }

    [Required]
    public double Height { get; set; } // Change to a numeric type

    [Required]
    public double Weight { get; set; } // Change to a numeric type

    public string? Message { get; set; }
        }




}
